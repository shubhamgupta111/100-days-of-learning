Q. Zero Ones Equal One Zeros
sol.
#include <bits/stdc++.h>
#include <string>
using namespace std;
int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    int n;
	    cin>>n;
	    string s[n];
	    for(int i=0;i<n;i++)
	    {
	        if(n%4==0)
	        {
	            if(i%4==0||(i+1)%4==0)
	            {
	            cout<<"1";
	            }
	            else
	            {
	                cout<<"0";
	            }
	        }
	        else
	        {
	            if(i==0||i==n-1)
	            {
	            cout<<"0";
	            }
	            else
	            {
	            cout<<"1";
	            }
	        }
	    }
	    cout<<endl;
	}
	return 0;
}
